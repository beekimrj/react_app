# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*

# Ignore uploaded files in development.
/storage/*
/public/assets

# Ignore master key for decrypting credentials and more.
/config/master.key
.env

# IDE files
.idea
/*.iml
.vscode

# React
/public/packs
/public/assets/packs
/public/packs-test
/public/assetz
/node_modules
yarn-debug.log*
.yarn-integrity
